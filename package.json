{
  "name": "devins-blog",
  "description": "",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run build:tailwind && sapper dev",
    "build": "npm run build:tailwind && sapper build --legacy",
    "export": "cross-env NODE_ENV=production npm run build:tailwind && sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "postcss static/tailwind.css -o static/index.css",
    "format": "prettier --write --svelte-sort-order scripts-markup-styles ./**/*.svelte"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "date-fns": "^1.30.1",
    "gray-matter": "^4.0.2",
    "lodash.pick": "^4.4.0",
    "marked": "^1.1.1",
    "polka": "^0.5.0",
    "prismjs": "^1.16.0",
    "reading-time": "^1.2.0",
    "sirv": "^1.0.1",
    "slugify": "^1.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@tailwindcss/typography": "^0.2.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.22.1",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.27.16",
    "svelte": "^3.0.0",
    "tailwindcss": "^1.5.1"
  }
}
